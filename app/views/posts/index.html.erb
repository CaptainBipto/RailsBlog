<h1>Listing posts</h1>
 
<table>
	<tr>
		<th>Title</th>
		<th>Content</th>
	</tr>
	
	<% @posts.each do |post|%>
		<tr>
			<td><%= post.title %></td>
			<td><%= post.text %></td>	
		</tr>
	<% end %>
</table>

<%= link_to "New Post", new_post_path %>
<%= link_to "Home", controller: "welcome" %>
